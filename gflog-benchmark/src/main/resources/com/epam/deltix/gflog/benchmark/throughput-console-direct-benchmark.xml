<config>

    <appender name="console" factory="com.epam.deltix.gflog.core.appender.ConsoleAppenderFactory" wrap="false">
        <layout template="%d %p '%c' [%t] %m%n"/>
    </appender>

    <logger>
        <appender-ref ref="console"/>
    </logger>

    <service entryEncoding="${encoding}"/>

</config>